{
    "private": true,
    "productName": "Entry",
    "name": "entry",
    "version": "2.0.31",
    "description": "Entry for offline",
    "main": "src/main_build/main.bundle.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development electron . --debug",
        "debug": "cross-env NODE_ENV=development electron --inspect .",
        "test": "mocha --require ts-node/register */**/*.spec.ts",
        "webpack:dev": "cross-env NODE_ENV=development webpack",
        "webpack:prod": "cross-env NODE_ENV=production webpack",
        "webpack:watch": "cross-env NODE_ENV=development webpack -w",
        "dist:win32": "cross-env NODE_ENV=production webpack && electron-builder -c --win --ia32",
        "dist:win64": "cross-env NODE_ENV=production webpack && electron-builder -c --win --x64",
        "dist:win": "cross-env NODE_ENV=production webpack && electron-builder -c --win --x64 && electron-builder -c --win --ia32",
        "dist:mac:notarize": "cross-env NODE_ENV=production webpack && cross-env NOTARIZE=true electron-builder -c --mac",
        "dist:mac": "cross-env NODE_ENV=production webpack && cross-env NOTARIZE=false electron-builder -c --mac",
        "rebuild": "electron-rebuild -f",
        "postinstall": "npm run rebuild && npm run webpack:dev"
    },
    "keywords": [
        "entry",
        "entry-offline"
    ],
    "author": "entrylabs",
    "license": "MIT",
    "dependencies": {
        "async-csv": "^2.1.3",
        "axios": "^0.19.2",
        "cross-spawn": "^7.0.3",
        "entry-hw": "file:../entry-hw/build-base",
        "entry-js": "file:../entryjs/build",
        "entry-tool": "git+https://github.com/entrylabs/entry-tool.git#v1.20200226.207",
        "excel4node": "^1.7.0",
        "fs-extra": "^8.1.0",
        "image-size": "^0.8.3",
        "jquery": "^3.4.1",
        "literallycanvas-mobile": "git+https://github.com/entrylabs/literallycanvas#feature/mobile",
        "lodash": "^4.17.15",
        "mime-types": "^2.1.26",
        "music-metadata": "^6.3.0",
        "node-xlsx": "^0.15.0",
        "puid": "^1.0.7",
        "rimraf": "^2.6.3",
        "serialport": "^9.0.4",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0",
        "tar": "^4.4.10",
        "uid": "^0.0.2",
